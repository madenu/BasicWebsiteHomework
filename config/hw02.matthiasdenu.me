server {
        listen 80;
        listen [::]:80;
        server_name hw02.matthiasdenu.me;
        root /home/matthias/www/hw02.matthiasdenu.me;
        index index.html;
        location / {
                # First attempt to serve request as file, then
                # as directory, then fall back to displaying a 404.
                try_files $uri $uri/ =404;
        }
}
